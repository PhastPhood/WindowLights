{
  "name": "textlights",
  "version": "1.0.0",
  "main": "",
  "author": "PhastPhood <jeffrey.gao@vanderbilt.edu>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run copy-static-assets",
    "serve": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "copy-static-assets": "node copyStaticAssets.js"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cron": "^1.2.1",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/mongodb": "^2.2.18",
    "@types/morgan": "^1.7.35",
    "concurrently": "^3.5.1",
    "nodemon": "^1.13.3",
    "request": "^2.83.0",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cron": "^1.3.0",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "googleapis": "^23.0.0",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "request": "^2.83.0",
    "shelljs": "^0.7.8",
    "swearjar": "^0.2.0"
  }
}
